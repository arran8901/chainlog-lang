on singleAssertExample: do assert(hello(abc)).

on singleRetractExample: do retract(abc).


test :-
  expect_msg(singleAssertExample, _, [assert(hello(abc))]),

  expect_msg(singleRetractExample, _, [retract(abc)]),

  expect_msg_fail(singleAssertExample(this_shouldnt_be_here), _,
                  error(match_error(msg_handler, _), _)),
  expect_msg_fail(nonExistentHandler, _,
                  error(match_error(msg_handler, _), _)),

